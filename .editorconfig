root = true

[*]
charset = utf-8
end_of_line = lf
max_line_length = 120
insert_final_newline = true
indent_size = 4
indent_style = space
ij_any_blank_lines_after_class_header = 0
ij_any_keep_blank_lines_before_right_brace = 0
ij_any_blank_lines_around_method = 1

[{*.har,*.json,*.png.mcmeta}]
indent_size = 2
tab_width = 2

[{*.yaml,*.yml}]
indent_size = 2
